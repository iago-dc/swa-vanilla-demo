{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
      {
        "route": "/*",
        "allowedRoles": [ "authenticated" ]
      }
    ],
     "responseOverrides": {
       "401": {
         "statusCode": 302,
         "redirect": "/.auth/login/github"
       }
        "400": {
            "statusCode": 302,
            "redirect": "/.auth/login/github"
        }
     }
}
